<ul class="roundalls-section" role="list">
    {%- for block in section.blocks -%}
        <li class="roundalls-section-list-item">
            <a href="{{ block.settings.link }}">
                {{ block.settings.name }}    
            </a>
        </li>
    {%- endfor -%}
</ul>

{% schema %}
{
    "name": "roundalls",
    "class": "section",
    "tag": "section",
    "settings": [
        {
            "type": "text",
            "id": "title",
            "label": "t:sections.multicolumn.title.name",
            "info": "t:sections.multicolumn.title.info"
        },
        {
            "type": "header",
            "content": "t:sections.all.padding.section_padding_heading"
        },
        {
            "type": "range",
            "id": "padding_top",
            "min": 0,
            "max": 100,
            "step": 4,
            "unit": "px",
            "label": "t:sections.all.padding.padding_top",
            "default": 36
        },
        {
            "type": "range",
            "id": "padding_bottom",
            "min": 0,
            "max": 100,
            "step": 4,
            "unit": "px",
            "label": "t:sections.all.padding.padding_bottom",
            "default": 36
        }
    ],
    "blocks": [
        {
            "type": "column",
            "name": "t:sections.multicolumn.blocks.column.name",
            "settings": [
                {
                    "type": "text",
                    "id": "name",
                    "default": "Image Description",
                    "label": "t:sections.faq_collapsible_content.blocks.collapsible_row.settings.heading.label"
                },
                {
                    "type": "url",
                    "id": "link",
                    "label": "t:sections.multicolumn.blocks.column.settings.link.label"
                }
            ]
        }
    ],
    "presets": [
        {
            "name": "roundalls",
            "blocks": [
                {
                    "type": "column"
                },
                {
                    "type": "column"
                }
            ]
        }
    ]
}
{% endschema %}