<script defer>
    new Promise(async function(resolve, reject) {
        while (!loadStatus.dom) {
            consoleMsg("waiting for enable dom", null, null);
            await delay(500);
        }

        loadJS("account_address_book", "{{ 'account-address-book.js' | asset_url }}", "account_address_book_loaded", true);
        resolve(true);
    });
</script>

